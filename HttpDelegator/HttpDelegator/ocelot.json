      "DownstreamPathTemplate": "/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5123"
        },
        {
          "Host": "localhost",
          "Port": "5246"
        }
